+		// bit[6] : otg disable
+		unsigned otg_disable:1;
+		// bit[8:7] : reserved
+		unsigned rsvd8_7:2;
+		// bit[9] : external vbus valid indicator (to phy)
+		unsigned vbusvld_ext:1;
+		// bit[10] : external vbus valid select
+		unsigned vbusvld_extsel:1;
+		// bit[31:11] : reserved
