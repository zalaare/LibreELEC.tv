. config/functions

# sector assignments per device
  case $UBOOT_SYSTEM in
    odroid_u2|odroid_u3)    BL1="1"; BL2="31"; U_BOOT="63"; TRUST_ZONE="2111" ;;
    odroid_xu3|odroid_xu4)  BL1="1"; BL2="31"; U_BOOT="63"; TRUST_ZONE="1503" ;;
  esac
