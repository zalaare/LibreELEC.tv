+static void exynos_usb_free(u32 uAddr)
+{
+	u32 uFree_addr;
+	if (uAddr != 0)
+	{
+		uFree_addr = *(u32 *)(uAddr - 4);
+		free((u8 *)uFree_addr);
+	}
+	DBG_USBD3("\n\nexynos_usb_free:0x%08x[0x%8x]\n\n", uAddr, uFree_addr);
+}
