+/*---------------------------------------------------------------------------*/
+static void odroid_gpio_deinit(void)
+{
+	gpio_direction_output(GPIO_FAN_CTL, 0);
+	gpio_direction_output(GPIO_LCD_PWM, 0);
+
+	gpio_free(GPIO_POWER_BT);
+	gpio_free(GPIO_FAN_CTL);
+	gpio_free(GPIO_LCD_PWM);
