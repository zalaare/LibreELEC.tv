+		unsigned force_sleep:1;
+		// bit[1] : force suspend
+		unsigned force_suspend:1;
+		// bit[2] : dmpulldown
+		unsigned dm_pulldown:1;
+		// bit[3] : dppulldown
+		unsigned dp_pulldown:1;
+		// bit[4] : drvvbus
