+	pTrb->buf_ptr_h = 0;
+
+	pTrb->status.data = 0;
+	pTrb->status.b.buf_siz = uLen;
+
+	pTrb->control.data = uTrbCtrl & 0xfffffffeU;
+
+	// must do this last => why?
+	if (uHwo)
+	{
+		pTrb->control.b.hwo = 1;
+	}
+
+}
