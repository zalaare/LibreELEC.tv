+
+/*---------------------------------------------------------------------------*/
+int odroid_get_partition_info(const char *ptn, struct partition_info *pinfo)
+{
+	int i;
+
+	for (i = 0; i < PART_MAX; i++) {
+		if (!strncmp(gPartInfo[i].name, ptn, strlen(ptn)))
+			break;
+	}
+	if (PART_MAX == i)
+		return	-1;
+
+	memcpy((void *)pinfo, (void *)&gPartInfo[i],
+		sizeof(struct partition_info));
+	return	0;
+}
